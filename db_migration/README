select public.oembed_providerrule.id, old_public.oembed_providerrule.id, public.oembed_providerrule.name from public.oembed_providerrule LEFT JOIN old_public.oembed_providerrule ON public.oembed_providerrule.id=old_public.oembed_providerrule.id;
grical_db=# select public.oembed_providerrule.id, old_public.oembed_providerrule.id, public.oembed_providerrule.name from public.oembed_providerrule LEFT JOIN old_public.oembed_providerrule ON public.oembed_providerrule.name=old_public.oembed_providerrule.name;


